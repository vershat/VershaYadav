#include <iostream>
using namespace std;

int main() {
    int a;
    cin >> a;

    if (a % 2 == 0) {
        a = a - 1;
    }

    int count = (a + 1) / 2;
    int num = 1;

    for (int i = 0; i < count; i++) {
        cout << num;
        if (i < count - 1) {
            cout << ", ";
        }
        num += 2;
    }

    return 0;
}
