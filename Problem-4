#include <iostream>
#include <vector>
#include <map>
using namespace std;

int main() {
    vector<int> nums = {1, 2, 8, 9, 12, 46, 76, 82, 15, 20, 30};
    map<int, int> result;

    for (int i = 1; i <= 9; i++) {
        int count = 0;
        for (int num : nums) {
            if (num % i == 0) {
                count++;
            }
        }
        result[i] = count;
    }

    cout << "{ ";
    for (auto it : result) {
        cout << it.first << ": " << it.second << " ";
    }
    cout << "}";

    return 0;
}
